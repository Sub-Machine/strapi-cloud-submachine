{
  "kind": "collectionType",
  "collectionName": "collections",
  "info": {
    "singularName": "collection",
    "pluralName": "collections",
    "displayName": "collection",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "render_hint": {
      "type": "enumeration",
      "enum": [
        "Large",
        "Standard",
        "Small"
      ],
      "required": true,
      "default": "Large"
    },
    "tiles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tile.tile"
    },
    "taxonomy": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::taxonomy.taxonomy"
    }
  }
}
